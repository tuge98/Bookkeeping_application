<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wellbeing">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Bookkeeping"
        android:usesCleartextTraffic="true">
        <activity android:name="com.example.wellbeing.carbonfootprintSummary"></activity>
        <activity android:name="com.example.wellbeing.calculatecarbonfootPrint" />
        <activity android:name="com.example.wellbeing.WeightScreen" />
        <activity android:name="com.example.wellbeing.menu" />
        <activity android:name="com.example.wellbeing.InsertInfoScreen" />
        <activity android:name="com.example.wellbeing.NutritionInfo" />
        <activity android:name="com.example.wellbeing.AddUserInfo" />
        <activity
            android:name="com.example.wellbeing.login"
            android:label="healthapp"
            android:theme="@style/Theme.Bookkeeping.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.wellbeing.register"
            android:label="healthapp" />
    </application>

</manifest>